<Window x:Class="Rudo.GameOverWindo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rudo"
        mc:Ignorable="d"
        Title="GameOverWindo" Height="450" Width="600">
    <Grid>
        <Label Content="GAME OVER" FontSize="20" FontWeight="bold" HorizontalAlignment="Left" Margin="245,35,0,0" VerticalAlignment="Top" Width="151" Height="44"/>
        <Label x:Name="gameOverLabel" FontSize="20" FontWeight="SemiBold" FontStyle="Italic" HorizontalAlignment="Left" Margin="155,101,0,0" VerticalAlignment="Top" Width="324" Height="123"/>
        <Button x:Name="historique" Content="Historique" HorizontalAlignment="Left" Margin="72,268,0,0" VerticalAlignment="Top" Width="98" Height="34" Click="historique_Click"/>
        <Button x:Name="playAgain" Content="Play Again " HorizontalAlignment="Left" Margin="290,268,0,0" VerticalAlignment="Top" Width="90" Height="34" Click="playAgain_Click"/>
        <Button x:Name="cancel" Content="Cancel" HorizontalAlignment="Left" Margin="460,268,0,0" VerticalAlignment="Top" Width="87"  Height="34" Click="cancel_Click"/>
    </Grid>
</Window>
